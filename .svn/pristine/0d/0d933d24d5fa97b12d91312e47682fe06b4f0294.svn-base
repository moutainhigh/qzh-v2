server:
  port: 8200
eureka:
  instance:
    hostname: register-server1
  client:
    serviceUrl:
      defaultZone: http://admin:admin@127.0.0.1:8200/eureka/
    registerWithEureka: false
    fetchRegistry: false
  server:
    enableSelfPreservation: false #服务端关闭自我保护模式
security:
  basic:
    enabled: true
  user:
    name: admin
    password: admin
#log
logging:
  level:
    com:
      huidong:
        qzh: warn
#          mapper: error
  file: logs/qzh-portal.log