---
server:
  port: 8700
management:
  security:
    enabled: false

security:
  user:
    name: admin
    password: admin
#  basic:
#    enabled: false
---
logging:
  file: "logs/qzh-admin-server.log"

spring:
  application:
    name: qzh-admin-server
  boot:
    admin:
      routes:
       endpoints: env,metrics,trace,dump,jolokia,info,configprops,trace,logfile,refresh,flyway,liquibase,heapdump,hystrix.stream
---
eureka:
  instance:
    hostname: qzh-sso
    prefer-ip-address: true
  client:
    serviceUrl:
      defaultZone: http://admin:admin@localhost:8200/eureka/



