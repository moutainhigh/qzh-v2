server:
  port: 8500
spring:
  application:
    name: qzh-ueditor
  http:
    multipart:
      max-file-size: 2000Mb #文件上传大小限制
      max-request-size: 2500Mb #文件下载大小限制
---
# log
logging:
  level:
    com:
      qzh: debug
  file: logs/qzh-ueditor.log
---
#注册中心
eureka:
  instance:
    hostname: qzh-ueditor
    prefer-ip-address: true
  client:
    serviceUrl:
      defaultZone: http://admin:admin@localhost:8200/eureka/